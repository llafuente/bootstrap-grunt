{
  "name": "bootstrap-grunt",
  "version": "0.0.1",
  "description": "bootstrap-grunt help you to organize your Gruntfile, splitting it into small files.",
  "main": "./index.js",
  "keywords": [
    "grunt",
    "bootstrap",
    "configuration",
    "scaffolding"
  ],
  "directories": {
    "lib": "."
  },
  "scripts": {
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/llafuente/bootstrap-grunt.git"
  },
  "author": {
    "name": "Luis Lafuente",
    "email": "llafuente@noboxout.com",
    "url": "http://www.noboxout.com"
  },
  "license": "MIT",
  "engine": {
    "node": ">=0.6"
  },
  "dependencies": {
  },
  "devDependencies": {
  },
  "jshintConfig": {
    "white": true,
    "curly": true,
    "eqeqeq": true,
    "indent": 4,
    "asi": false,
    "boss": false,
    "browser": true,
    "devel": true,
    "maxdepth": 4,
    "maxstatements": 25,
    "maxcomplexity": 6,
    "eqnull": true,
    "es5": true,
    "evil": false,
    "immed": false,
    "jquery": true,
    "latedef": false,
    "laxcomma": true,
    "laxbreak": true,
    "newcap": true,
    "node": true,
    "noempty": true,
    "nonew": true,
    "predef": [
      "log"
    ],
    "quotmark": "double",
    "smarttabs": false,
    "strict": false,
    "trailing": true,
    "undef": true,
    "unused": true
  }
}
